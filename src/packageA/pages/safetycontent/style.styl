.safety-content{
  width: 100%;
  height: 100vh;
  overflow-y: auto;
  background-color: #FFF;
  padding: 10px 10px 80px 10px;
  .label{
    font-style: normal;
    font-size: 26px;
    font-weight: 700;
    margin: 12px 0;
  }
  .content2{
    font-style: normal;
    font-weight: 400;
    width: 95%;
    padding: 5px 10px;
  }
}
.safetycontent-fixed {
  position fixed
  bottom 40px
  left 50%
  transform translateX(-50%)
}

.image-readonly {
  .at-image-picker__preview-img {
    border: 1px solid #e6e6e6;
  }
  .at-image-picker__remove-btn {
    display none
  }
}
